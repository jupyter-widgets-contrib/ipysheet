{
  "name": "ipysheet",
  "version": "0.3.2",
  "description": "Spreadsheet in the Jupyter notebook",
  "author": "Maarten A. Breddels",
  "main": "lib/src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuantStack/ipysheet.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project . && webpack --mode=production",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w --project .",
    "watch:nbextension": "webpack --watch --mode=development",
    "test": "karma start --single-run"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^2.2.41",
    "chai": "^4.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.15.0",
    "karma": "^4.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^3.0.9",
    "karma-typescript-es6-transform": "^1.0.3",
    "karma-webpack": "^3.0.5",
    "mocha": "^3.3.0",
    "npm-run-all": "^4.1.3",
    "puppeteer": "^1.12.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.11.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.18.2",
    "typescript": "^3.3.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.0.1",
    "@jupyter-widgets/controls": "^1.0.1",
    "handsontable": "^6.2.2",
    "lodash": "^4.17.4",
    "underscore": "^1.8.3"
  }
}
