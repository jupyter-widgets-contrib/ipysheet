{
  "name": "ipysheet",
  "version": "0.4.3",
  "description": "Spreadsheet in the Jupyter notebook",
  "author": "Maarten A. Breddels",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/QuantStack/ipysheet.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project . && webpack --mode=production",
    "watch": "npm-run-all -p watch:*",
    "watch:lib": "tsc -w --project .",
    "watch:nbextension": "webpack --watch --mode=development",
    "test": "karma start --single-run"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "karma": "^5.0.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^5.0.2",
    "karma-typescript-es6-transform": "^5.0.2",
    "karma-webpack": "^4.0.2",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2.0 || ^3.0",
    "handsontable": "^6.2.2",
    "lodash": "^4.17.4",
    "underscore": "^1.8.3"
  }
}
